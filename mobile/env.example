# Mobile App Environment Variables

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8000
EXPO_PUBLIC_APP_NAME=FootyBets.ai
EXPO_PUBLIC_VERSION=1.0.0

# Expo Configuration
EXPO_PUBLIC_PROJECT_ID=your-expo-project-id-here

# Push Notifications
EXPO_PUBLIC_PUSH_TOKEN=your-push-token-here

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ID=your-analytics-id-here

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRICS=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Development
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info 